{
    "appName": "My Recipe Book",
    "branding": {
      "primaryColor": "#4CAF50",
      "accentColor": "#A5D6A7",
      "backgroundColor": "#F8F8F8",
      "textColor": "#333333",
      "fontFamily": "Inter, system-ui, sans-serif",
      "borderRadius": "16px",
      "elevation": "medium",
      "spacingUnit": "8px"
    },
    "components": {
      "buttons": {
        "shape": "pill",
        "textTransform": "none",
        "primary": {
          "backgroundColor": "#4CAF50",
          "textColor": "#FFFFFF"
        },
        "secondary": {
          "backgroundColor": "#FFFFFF",
          "textColor": "#4CAF50",
          "border": "1px solid #4CAF50"
        }
      },
      "cards": {
        "backgroundColor": "#FFFFFF",
        "borderRadius": "16px",
        "shadow": true,
        "padding": "16px",
        "imageCornerStyle": "rounded"
      },
      "inputs": {
        "backgroundColor": "#FFFFFF",
        "borderRadius": "12px",
        "padding": "12px 16px",
        "fontSize": "16px"
      },
      "searchBar": {
        "placeholder": "Search any recipe...",
        "icon": "search",
        "backgroundColor": "#FFFFFF",
        "borderRadius": "16px",
        "padding": "12px 16px"
      },
      "tabs": {
        "style": "pill",
        "activeColor": "#4CAF50",
        "inactiveColor": "#DDDDDD"
      },
      "bottomNav": {
        "backgroundColor": "#FFFFFF",
        "iconColor": "#4CAF50",
        "activeIndicatorColor": "#4CAF50"
      }
    },
    "images": {
      "style": "rounded",
      "shadow": true,
      "aspectRatio": "4:3"
    },
    "layout": {
      "containerPadding": "16px",
      "sectionSpacing": "24px",
      "cardSpacing": "12px"
    },
    "typography": {
      "h1": {
        "fontSize": "24px",
        "fontWeight": "600"
      },
      "h2": {
        "fontSize": "20px",
        "fontWeight": "600"
      },
      "body": {
        "fontSize": "16px",
        "fontWeight": "400"
      },
      "caption": {
        "fontSize": "12px",
        "color": "#777777"
      }
    },
    "iconography": {
      "style": "outline",
      "size": "24px"
    },
    "theme": {
      "lightMode": true,
      "darkMode": false
    }
  }
  